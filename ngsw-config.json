{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/media/**"
                ]
            }
        }
    ],
    "navigationRequestStrategy": "freshness"
}